<strong>
{{#each subjects}}
{{this}}, 
{{/each}}
</strong>

<h3>{{ title }}</h3>
<strong>{{ provider }}</strong>
<p>{{ description }} </p>

<div id="authStatus"></div>

<ul>
{{#presentations}}
<li>
	{{#if start }}
	Starts: {{ dateFormat start }}
	{{/if}}
    <span class="bookable">
    {{#if booking_endpoint }}
	<a class="bookLink" data-id="{{ id }}">Book</a>
    {{else}}
    (Cannot be booked at the moment.)
    {{/if}}
    </span>
</li>
{{/presentations}}
</ul>